local r177_1 = function()
	local r0_169 = wait()
	if not r0_169 then
		if _G.AutoCollectEgg then
			local eggs = workspace.Map.PrehistoricIsland.Core.SpawnedDragonEggs:GetChildren()
			if #eggs > 0 then
				local r1_169 = math.random(1, #eggs)
				local targetEgg = eggs[r1_169]
				
				if targetEgg:IsA("Model") and targetEgg.PrimaryPart then
					Tween2(targetEgg.PrimaryPart.CFrame)
					if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - targetEgg.PrimaryPart.Position).Magnitude <= 5 then
						local r5_169 = game:GetService("VirtualInputManager")
						r5_169:SendKeyEvent(true, "E", false, game)
						wait(0.5)
						r5_169:SendKeyEvent(false, "E", false, game)
					end
				end
			end
		end
	end
end
spawn(r177_1)
