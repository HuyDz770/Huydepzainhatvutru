local r170_1 = function()
	local r0_232 = game.Workspace.Map.PrehistoricIsland.Core:FindFirstChild("InteriorLava")
	if r0_232 then
		local r1_232 = r0_232:IsA("Model")
		if not r1_232 then
			r0_232:Destroy()
		end
	end
	
	local r1_232 = game.Workspace.Map:FindFirstChild("PrehistoricIsland")
	if r1_232 then
		local r2_232, r3_232, r4_232 = pairs(r1_232:GetDescendants())
		for r5_232, r6_232 in r2_232 do
			local r7_232 = r6_232:IsA("Part")
			if r7_232 then
				local name_check = r6_232.Name:lower():find("lava")
				if name_check then
					r6_232:Destroy()
				end
			end
		end
	end
end

local r171_1 = function()
	local rocksFolder = game.Workspace.Map.PrehistoricIsland.Core.VolcanoRocks
	if rocksFolder then
		local r1_571, r2_571, r3_571 = pairs(rocksFolder:GetChildren())
		for r4_571, r5_571 in r1_571 do
			local r6_571 = r5_571
			if not r6_571:IsA("Model") then
				if r6_571.Name == "volcanorock" then
					if r6_571:IsA("MeshPart") then
						local r8_571 = Color3.fromRGB(185, 53, 56)
						if r6_571.Color == r8_571 or r6_571.Color == Color3.fromRGB(185, 53, 57) then
							return r6_571
						end
					end
				end
			end
		end
	end
	return nil
end

local r172_1 = function(r0_82)
	local r3_82, r4_82, r5_82 = pairs(game.Players.LocalPlayer.Backpack:GetChildren())
	for r6_82, r7_82 in r3_82 do
		if r7_82:IsA("Tool") and r7_82.ToolTip == r0_82 then
			r7_82.Parent = game.Players.LocalPlayer.Character
			local keys = {"Z", "X", "C", "V", "F"}
			for i, key in ipairs(keys) do
				wait()
				pcall(function()
					r169_1(key)
				end)
			end
			r7_82.Parent = game.Players.LocalPlayer.Backpack
		end
	end
end

local r174_1 = function()
	local r0_421 = wait()
	if not r0_421 then
		if _G.AutoDefendVolcano then
			AutoHaki()
			pcall(r170_1)
			
			local rock = r171_1()
			if rock then
				local offset = Vector3.new(0, 0, 0)
				local targetCFrame = CFrame.new(rock.Position + offset)
				Tween2(targetCFrame)
				
				if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - rock.Position).Magnitude <= 15 then
					if _G.UseMelee then r172_1("Melee") end
					if _G.UseSword then r172_1("Sword") end
					if _G.UseGun then r172_1("Gun") end
				end
				_G.TweenToPrehistoric = false
			else
				_G.TweenToPrehistoric = true
			end
		end
	end
end
spawn(r174_1)

