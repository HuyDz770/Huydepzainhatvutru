local r175_1 = function()
	local r0_476 = wait()
	if not r0_476 then
		if KillAura then
			local r1_476 = function()
				local r0_477, r1_477, r2_477 = pairs(game.Workspace.Enemies:GetDescendants())
				for r3_477, r4_477 in r0_477 do
					local r5_477 = r4_477:FindFirstChild("Humanoid")
					if r5_477 then
						if r5_477.Health > 0 and (r4_477.Name == "Golem" or r4_477.Name == "Magma Golem") then
							local hrp = r4_477:FindFirstChild("HumanoidRootPart")
							if hrp and (hrp.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 50 then
                                sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
								r5_477.Health = 0
								hrp.CanCollide = false
							end
						end
					end
				end
			end
			pcall(r1_476)
		end
	end
end
spawn(r175_1)

